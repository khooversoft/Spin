{
  "$include-secret": "file://{environment}-Spin.secret.json",
  "$include-resources": "file://{environment}-Spin.resources.json",

  "storage": [
    {
      "channel": "{channel}",
      "accountName": "{accountName}",
      "containerName": "{contianerName}",
      "accountKey": "{accountKey}",
      "pathRoot": "V1.0/{path}"
    }

  ],
  "queue": [
    {
      "channel": "{channel}",
      "autoComplete": false,
      "maxConcurrentCalls": 10,
      "serviceBus": {
        "namespace": "{namespace}",
        "name": "{queueName}",
        "authSendListen": "{auth-string}",
        "authManage": "{auth-string}"
      }
    }
  ]
}
