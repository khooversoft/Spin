  "Store": {
    "Namespaces": {

      "dev-identity": {
        "Namespace": "identity",
        "PathRoot": "V1.0",
        "Store": {
          "AccountName": "devartifactstorage",
          "ContainerName": "dev-identity",
          "AccountKey": "{devartifactstorage.AccountKey}"
        }
      },

      "dev-customer": {
        "Namespace": "customer",
        "PathRoot": "V1.0",
        "Store": {
          "AccountName": "devartifactstorage",
