{
  "ApiKey": "{local.apikey}",

  "Store": {
    "Namespaces": {

      "dev-identity": {
        "Namespace": "identity",
        "PathRoot": "V1.0",
        "Store": {
          "AccountName": "devartifactstorage",
          "ContainerName": "local-identity",
          "AccountKey": "{devartifactstorage.AccountKey}"
        }
      },

      "dev-customer": {
        "Namespace": "customer",
        "PathRoot": "V1.0",
        "Store": {
          "AccountName": "devartifactstorage",
          "ContainerName": "local-customer",
          "AccountKey": "{devartifactstorage.AccountKey}"
        }
      },

      "dev-smart-contract": {
        "Namespace": "smart-contract",
        "PathRoot": "V1.0",
        "Store": {
          "AccountName": "devartifactstorage",
          "ContainerName": "local-smart-contract",
          "AccountKey": "{devartifactstorage.AccountKey}"
        }
      },

      "dev-directory": {
        "Namespace": "directory",
        "PathRoot": "v1.0",
        "Store": {
          "AccountName": "devartifactstorage",
          "ContainerName": "local-directory",
          "AccountKey": "{devartifactstorage.AccountKey}"
        }
      }
    }
  }
}
