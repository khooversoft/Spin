<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
      <!--<UserSecretsId>06458f66-fe96-42b3-8545-aea890b3159d</UserSecretsId>-->
    <!-- Enable user secrets in Development -->
    <UserSecretsId>TicketShareWeb-Dev-Secrets</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <!-- Apple ID OpenID Connect provider (aspnet-contrib) -->
    <PackageReference Include="AspNet.Security.OAuth.Apple" Version="8.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.OpenIdConnect" Version="9.0.8" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Tools\Toolbox\Toolbox.csproj" />
  </ItemGroup>

</Project>
